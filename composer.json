{
    "name": "daveismyname/laravel-box",
    "description": "A Laravel Box package",
    "license": "MIT",
    "authors": [
        {
            "name": "David Carr",
            "email": "dave@daveismyname.com",
            "homepage": "https://daveismyname.blog"
        }
    ],
    "homepage": "https://github.com/daveismyname/laravel-box",
    "keywords": ["Laravel", "Box"],
    "require": {
        "illuminate/support": "~5",
        "guzzlehttp/guzzle": "~6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Daveismyname\\Box\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daveismyname\\Box\\BoxServiceProvider"
            ],
            "aliases": {
                "Box": "Daveismyname\\Box\\Facades\\Box"
            }
        }
    }
}
